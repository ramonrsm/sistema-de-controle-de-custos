{
  "name": "backend",
  "version": "1.0.0",
  "description": "Sistema De Controle De Custos API",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev ts-node --transpile-only server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "typeorm"
  ],
  "author": "Ramon Moura <https://github.com/ramonrsm>",
  "homepage": "https://github.com/ramonrsm/sistema-de-controle-de-custos/blob/master/README.md",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.14",
    "cross-env": "^7.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29"
  }
}
